{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": { 
        "react/jsx-filename-extension": [0],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4]
     },
    "env": {
        "browser": true
    },
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src"]
          }
        }
      }
}
